
<%- include('partials/header'); -%>
<div class="compose">
  <button type="submit" class="btn btn-primary" name="button" onclick="window.location.href='/compose';">compose</button>
</div>
<h1>Home</h1>
<p> <%= startingContent %></p>
<% posts.forEach(function(post) { %>
